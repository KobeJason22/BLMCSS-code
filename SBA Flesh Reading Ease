program project1;

uses SysUtils;

var
  inputStr, line : string;
  score: real;
  i, k: integer;

begin
  writeln('Input the essay: ');
  readln(inputStr);
  k:=1;
  for i:=1 to length(inputStr) do
  begin
   if(inputStr[i] = ' ')
      then k:=k+1;
  end;
  writeln(k);


  Readln;
end.                   
